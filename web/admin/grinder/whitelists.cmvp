<CENTER>
<FORM  ACCEPT-CHARSET="UTF-8" NAME=WHITE_IPCONN TARGET=_top METHOD=POST ACTION="whitelists.cmvp">
	<INPUT TYPE=HIDDEN NAME=AUTH VALUE="@Authenticate?AUTH@">
	<CENTER><FONT COLOR=RED><B>@WhiteListMgr?DELETE&IPCONN@ @WhiteListMgr?ADD&IPCONN@</B></FONT></CENTER>
	<BR>
	<TABLE WIDTH=100% BORDER=1>
	<TR><TD WIDTH=100% COLSPAN=4 BGCOLOR=WHITE ALIGN=CENTER><FONT COLOR=BLACK><H2>IP Address Whitelist Management</H2></FONT></TD></TR>
	@WhiteListMgr?RESET&IPCONN@
	@loop@
	<TR>
	@WhiteListMgr?NEXT&IPCONN@<TD WIDTH=33%><INPUT TYPE=RADIO NAME=BANNEDONE_IPCONN VALUE="@WhiteListMgr?IPCONN@"><FONT COLOR=WHITE><B>@WhiteListMgr?IPCONN@</B></FONT></TD>
	@WhiteListMgr?NEXT&EMPTYOK&IPCONN@<TD WIDTH=33%><INPUT TYPE=RADIO NAME=BANNEDONE_IPCONN VALUE="@WhiteListMgr?IPCONN@"><FONT COLOR=WHITE><B>@WhiteListMgr?IPCONN@</B></FONT></TD>
	@WhiteListMgr?NEXT&EMPTYOK&IPCONN@<TD WIDTH=33%><INPUT TYPE=RADIO NAME=BANNEDONE_IPCONN VALUE="@WhiteListMgr?IPCONN@"><FONT COLOR=WHITE><B>@WhiteListMgr?IPCONN@</B></FONT></TD>
	</TR>
	@back@
	<TR>
	<TD WIDTH=100% COLSPAN=3><FONT COLOR=WHITE><B>Add New: </B></FONT><INPUT TYPE=TEXT SIZE=50 MAXLENGTH=255 NAME=NEWBANNEDONE1></TD>
	</TR>
	</TABLE>
	<P>
	<TABLE WIDTH=100% BORDER=0>
	<TR>
	<TD WIDTH=50% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=DELGO VALUE="Delete selected" ONCLICK="ClearClicked('/grinder/whitelists.cmvp','1');">
	</TD>
	<TD WIDTH=50% ALIGN=CENTER>
	<INPUT TYPE=BUTTON NAME=ADDNEW VALUE="Add New" ONCLICK="Clicked('/grinder/whitelists.cmvp','1');">
	</TD>
	</TABLE>
</FORM>
